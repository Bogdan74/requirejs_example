
// https://gist.github.com/amatiasq/db597053f0f891ff7abc

{
  "nonew": true,
  "curly": true,
  "latedef": true,
  "noarg": true,
  "esnext": true,
  "forin": true,
  "noempty": true,
  "node": true,
  "eqeqeq": true,
  "strict": true,
  "undef": true,
  "bitwise": true,
  "newcap": true,
  "onevar": true,
  "camelcase": true,
  "nonbsp": true,
  "freeze": true,
  "browser": true,
  "globals"   : {
    "define"   : false,
    "requirejs": false,
    "$"        : false,
    "bt"       : false,
    "_gaq"     : false
  },


  // Ignored Warnings
  "-W069": true
//  "-W026": true,
//  "-W003": true   // 'x' was used before it was defined
  //"-W013": true,   // Missing space after 'x'. It fires with "function() { }"
  //"-W070": true,   // Extra comma. (it breaks older versions of IE)
  //"-W026": true    // Inner functions should be listed at the top of the outer function.

}
